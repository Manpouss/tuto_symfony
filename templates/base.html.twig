        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        <header class="jumbotron">
            <nav>
                <a href="{{ path('accueil') }}" class="btn btn-primary">Accueil</a>
                <a href="{{ path('article') }}" class="btn btn-primary">Article</a>
                {# Les accolades dans le href du lien servent Ã  envoyer le chemin du lien#}

            </nav>
